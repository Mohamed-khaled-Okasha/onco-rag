{
    "builds": [
        {
            "src": "api/analyze.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "30mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/analyze",
            "dest": "/api/analyze.py"
        },
        {
            "src": "/api/(.*)",
            "dest": "/api/analyze.py"
        }
    ]
}